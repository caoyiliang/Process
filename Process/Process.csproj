<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard2.0;net8.0</TargetFrameworks>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<LangVersion>default</LangVersion>
		<PackageId>CSoft.Process</PackageId>
		<Title>流程控制</Title>
		<Version>1.0.0</Version>
		<Authors>CSoft</Authors>
		<PackageIcon>logo.png</PackageIcon>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<RepositoryUrl>https://github.com/caoyiliang/Process</RepositoryUrl>
		<PackageProjectUrl>https://github.com/caoyiliang/Process</PackageProjectUrl>
	</PropertyGroup>

	<ItemGroup>
	  <None Include="..\logo.png">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="CSoft.DataPairs.Newtonsoft" Version="8.0.2" />
		<PackageReference Include="CSoft.LogInterface" Version="8.0.0" />
	</ItemGroup>

</Project>
